    "version": "0.2.0",
    "configurations": [
        {
            "type": "gdb",
            "request": "launch",
            "name": "Launch Program",
            "target": "./program.o",
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText",
            "env": {"JAVA_HOME": "/usr/lib/jvm/jdk1.7.0_80"},
            "autorun": ["handle SIGPWR nostop noprint", "handle SIGXCPU nostop noprint"]
        }
    ]
}